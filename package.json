{
  "name": "greensms",
  "version": "1.0.3",
  "description": "GREENSMS API: SMS, Viber, Voce, Call, HLR, Pay",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "directories": {
    "example": "examples",
    "lib": "lib"
  },
  "files": [
    "dist",
    "lib"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "_watch": "rollup -c -w",
    "_lint_global": "jshint ./lib",
    "_lint": "node ./node_modules/jshint/dist/jshint.js ./lib",
    "_jsdoc": "node ./node_modules/jsdoc/jsdoc.js -c jsdoc.conf.json",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "GREENSMS_USER=test GREENSMS_PASS=test nyc mocha --timeout 10000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greensms-ru/greensms-node.git"
  },
  "keywords": [
    "greensms",
    "sms",
    "rest",
    "api"
  ],
  "author": "GREENSMS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/greensms-ru/greensms-node/issues"
  },
  "homepage": "https://github.com/greensms-ru/greensms-node#readme",
  "dependencies": {
    "axios": "^0.20.0",
    "humps": "^2.0.1",
    "jsonschema": "^1.3.0",
    "qs": "^6.9.4",
    "urijs": "^1.19.2",
    "yup": "^0.29.3"
  },
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "jsdoc": "^3.6.6",
    "jshint": "^2.12.0",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "rollup": "^2.32.0",
    "rollup-plugin-dts": "^1.4.13",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "supertest": "^5.0.0",
    "typescript": "^4.1.5"
  },
  "settings": {
    "moduleName": "greensms",
    "minify": false
  },
  "optionalDependencies": {
    "fsevents": "2.1.3"
  }
}
